#include "msp430.h" 
 ORG 0x3010
mystr DW 0x4444
 ORG 0x3020
mystr1 DW 0xAAAA
 NAME main 
 PUBLIC main 
 ORG 0FFFEh
 DC16 init 
 RSEG CSTACK 
 RSEG CODE 
init: MOV #SFE(CSTACK), SP 
main: NOP 
 MOV.W #WDTPW+WDTHOLD, &WDTCTL 
CLR R5
 MOV.W 0X3010(R5),R4 
 MOV.W 0X3020(R5),R6 
 SUB.W R4,R6
 MOV.W R6,0X3030(R5) 
 JMP $ 
 END
