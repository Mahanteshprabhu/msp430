#include "msp430.h"  
 NAME main 
ORG 0X3010
mystr DW 0X3333
 DW 0X1111
 DW 0X5555
 DW 0X2222
 DW 0XFFFF
 DW 0X8888
 DW 0X6666
 DW 0X7777
PUBLIC main  
 ORG 0FFFEh
 DC16 init  
 RSEG CSTACK  
 RSEG CODE  
init: MOV #SFE(CSTACK), SP  
main: NOP  
 MOV.W #WDTPW+WDTHOLD, & WDTCTL  
L4: CLR R5
 MOV.W #0X0007,R10
 MOV.W 0X3010(R5),R6
L3: INC R5
 INC R5
 CMP 0X3010(R5),R6
 JC L1
 JNE L2
 JMP L1
L2: MOV.W 0X3010(R5),R6
L1: DEC R10
 CMP #0X0000, R10
 JNE L3
 MOV.W R6, 0X3020(R10)
 JMP $ 
 END
